{
  "name": "thinkopenhack",
  "version": "1.0.0",
  "description": "#ThinkOpenHealth - Hackathon for Health",
  "repository": "https://github.com/itsacezon/thinkopenhack",
  "private": true,
  "scripts": {
    "watch-css": "node-sass -w ThinkOpenHealthApp/app/scss/build.scss static/css/build.css",
    "watch-js": "watchify -vd -p browserify-hmr -e ThinkOpenHealthApp/app/js/build.js -o static/js/build.js",
    "dev": "npm-run-all --parallel watch-css watch-js",
    "build-js": "cross-env NODE_ENV=production browserify ThinkOpenHealthApp/app/js/build.js | uglifyjs -c warnings=false -m > static/js/build.js",
    "build-css": "node-sass ThinkOpenHealthApp/app/scss/build.scss static/css/build.css | uglifycss --ugly-comments static/css/build.css > static/css/build.css",
    "build": "npm-run-all --parallel build-js build-css"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "dependencies": {
    "relative-date": "^1.1.3",
    "vue": "^1.0.21",
    "vue-resource": "^0.7.0",
    "vue-router": "^0.7.13"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^5.8.38",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.7",
    "http-server": "^0.9.0",
    "jade": "^1.11.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.1.1",
    "npm-run-all": "^1.7.0",
    "uglify-js": "^2.6.2",
    "uglifycss": "0.0.20",
    "vue-hot-reload-api": "^1.3.2",
    "vueify": "^8.3.9",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.7.0"
  }
}
